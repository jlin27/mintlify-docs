{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "0x",
  "colors": {
    "primary": "#25272a",
    "light": "#f3f4f5",
    "dark": "#25272a"
  },
  "appearance": {
    "default": "light"
  },
  "background": {
    "decoration": "gradient"
  },
  "fonts": {
    "heading": {
      "family": "PolySans",
      "weight": 400,
