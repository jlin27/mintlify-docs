              },
              {
                "group": "Additional Topics",
                "pages": [
                  "docs/0x-swap-api/advanced-topics/how-to-set-your-token-allowances",
                  "docs/0x-swap-api/advanced-topics/handling-native-tokens",  
                  "docs/0x-swap-api/advanced-topics/0x-parser",
                  "docs/0x-swap-api/advanced-topics/sell-entire-balance",
                  "docs/0x-swap-api/advanced-topics/buy-sell-tax-support",
                  "docs/0x-swap-api/advanced-topics/about-the-rfq-system"
                ]
              }
            ]
          },
          {
            "group": "Gasless API",
            "pages": [
              "docs/gasless-api/introduction",
              {
                  "group": "Guides",
                  "pages": [
                    "docs/gasless-api/guides/get-started-with-gasless-api",
                    "docs/gasless-api/understanding-gasless-api",
                    "docs/gasless-api/guides/gasless-api-technical-appendix"
                  ]
              },
              "docs/gasless-api/gasless-faq"
            ]
          },
          {
            "group": "Trade Analytics API",
            "pages": ["docs/trade-analytics-api/introduction"]
          }
